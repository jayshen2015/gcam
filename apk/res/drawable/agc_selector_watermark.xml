<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:gravity="center" android:height="16.0dip" android:width="16.0dip">
                <shape>
                    <solid android:color="?colorSecondary" />
                    <corners android:radius="16.0dip" />
                </shape>
            </item>
            <item android:gravity="center" android:height="12.0dip" android:width="12.0dip">
                <bitmap android:gravity="center" android:src="@drawable/agc_icon_checked" />
            </item>
        </layer-list>
    </item>
    <item>
        <shape>
            <solid android:color="#ff313131" />
            <stroke android:width="0.5dip" android:color="@color/white" />
            <corners android:radius="16.0dip" />
        </shape>
    </item>
</selector>